{
    "name": "lsbproject/request-bundle",
    "description": "Bundle to structure common request data",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "v1.2.1",
    "authors": [
        {
            "name": "victor.fedorenko",
            "email": "fedorenko22116@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^3.0 || ^4.0 || ^5.0",
        "sensio/framework-extra-bundle": "^3.0 || ^4.0 || ^5.0",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/class-loader": "^3.4",
        "phpstan/phpstan": "^0.12.14",
        "squizlabs/php_codesniffer": "^3.5",
        "escapestudios/symfony2-coding-standard": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "LSBProject\\RequestBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LSBProject\\RequestBundle\\Tests\\Unit\\": "/tests/Unit"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse src",
        "phpcs": "./vendor/bin/phpcs",
        "phpcbf": "./vendor/bin/phpcbf"
    }
}
